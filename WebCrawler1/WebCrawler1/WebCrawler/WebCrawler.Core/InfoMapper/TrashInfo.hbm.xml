<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WebCrawler.Core"  namespace="WebCrawler.Core.Info" xmlns="urn:nhibernate-mapping-2.2" >
  <class name="TrashInfo" table="tblTrash" >
    <meta attribute="class-description">
      This class contains the employee detail.
    </meta>
    <id name="TrashId" type="int" column="TrashId">
      <generator class="native"/>
    </id>
    <property name="TrashFolderName" column="TrashFolderName" type="string"/>
    <property name="TrashImageLink" column="TrashImageLink" type="string"/>
    <property name="TrashDate" column="TrashDate" type="Date"/>
  </class>
</hibernate-mapping>